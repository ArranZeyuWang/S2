{
  "name": "@antv/s2-react",
  "version": "0.1.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/s2.min.js",
  "types": "esm/index.d.ts",
  "description": "effective spreadsheet render lib",
  "license": "MIT",
  "homepage": "https://s2.antv.vision",
  "author": "https://github.com/orgs/antvis/people",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antvis/S2.git"
  },
  "bugs": {
    "url": "https://github.com/antvis/S2/issues"
  },
  "files": [
    "esm",
    "lib",
    "dist",
    "README.md"
  ],
  "keywords": [
    "antv",
    "s2",
    "s2-react",
    "spreadsheet",
    "pivot table"
  ],
  "scripts": {
    "start": "vite",
    "build": "yarn tsc && vite build --debug",
    "clean": "rimraf lib esm dist",
    "build:esm": "rollup -c rollup.config.js --environment FORMAT:esm ",
    "build:cjs": "rollup -c rollup.config.js --environment FORMAT:cjs",
    "build:umd": "rollup -c rollup.config.js --environment FORMAT:umd",
    "build:analysis": "rollup -c rollup.config.js --environment FORMAT:esm,ANALYSIS",
    "bundle:size": "bundlesize",
    "watch": "rimraf esm && FORMAT=es rollup -w -c rollup.config.js",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "tsc": "tsc --noEmit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "peerDependencies": {
    "@antv/s2": "^0.2.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@antv/s2": "^0.2.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  }
}
